.canvas{
  width: 95%;
  margin: 0 auto;
  height: 500rpx;
  position: absolute;
  top: 50rpx;
  left: 0;
  right: 0;
  bottom: 50rpx;
  background-color:#E9E9E9;
  text-align: left;
}
.list{
  margin: 40rpx 40rpx;
  float: left;
  padding: 20rpx 40rpx 20rpx 40rpx;
  background-color: #fff;
}
.pink{
  color: pink;
  float: left;
}
.hide{
 display: none;
}
.more{
 display: block;
}
.btn{
  float: right;
  margin-right: 50rpx;
  width: 300rpx;
  height: 80rpx;
  line-height: 80rpx;
  margin-top: 40rpx;
  border: 1rpx solid #E0E0E0;
  color: #7B7B7B;
}
.container{
 width: 95%;
 /*height: 800rpx;*/
 overflow-y:hidden;
 margin: 50rpx auto;
 overflow-x: hidden;
 padding-bottom: 40rpx;
 border: 1rpx solid #E0E0E0;
}
/*.scroll-image{
  width: 100%;
  height: 400rpx;
}*/
.content{
  background-color: #fff;
  width: 100%;
  text-align: center;
}
.title{
  height: 100rpx;
  line-height: 100rpx;
  font-size: 50rpx;
  color: #000;

}
.scroll-view-item-AT{
  width: 100%;
  font-size: 36rpx;
  color: #c9c;
}
.area{
  display: inline-block;
}
.day{
  margin-left: 10rpx;
  display: inline-block;
}
.fee{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
}

.scroll-view-item-cf{
  padding-top: 20rpx;
  width: 100%;
  height: 150rpx;
  position:relative;
}
.scroll-view-item-cf::before{
  content: '';
  position: absolute;
  top: 0;
  left: 10rpx;
  right: 10rpx;
  height: 1rpx;
  background-color: #E0E0E0;

}
.camerist_avatar{
  margin-top: 20rpx;
  margin-left: 20rpx;
  float: left;
  width: 100rpx;
  height: 100rpx;
}
.camerist_avatar image{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  vertical-align: middle;
}
.camerist{
  margin-top: 20rpx;
  float: left;
  width: 200rpx;
  height: 150rpx;
  line-height: 150rpx;
  vertical-align: middle;
  margin-left: 20rpx;
}
.camerist_name{
  font-size: 40rpx;
  height: 70rpx;
  line-height: 70rpx;
  text-align: left;
  color: #7B7B7B;
}
.experience{
  padding-left: 10rpx;
  padding-right: 10rpx;
  font-size: 30rpx;
  float: left;
  height: 40rpx;
  line-height: 40rpx;
  border: 1px solid  #E0E0E0;
  color: #7B7B7B;
}
.reserve{
 float: right;
 margin-top: 40rpx;
 font-size: 40rpx;
 width: 100%;
 height: 80rpx;
 line-height: 80rpx;
 border-radius:20rpx;
 background-color:#FF9797;
 color: white;
}
.reserve .tetx{
  background-color: green;
  width: 200rpx;
  height: 60rpx;
  font-size: 50rpx;
}
